@model IEnumerable<AJCCFM.Models.Service.JDEProgress>

@{
    Layout = null;
}



        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-hover table-striped" id="JDEProgressGrid">
                    <thead>
                        <tr>
                            <th>RefNo</th>
                            <th>Requested On</th>
                            <th>Requested By</th>

                            <th></th>


                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>


                            <td>
                                <b>
                                    @Html.DisplayFor(modelItem => item.RefNo)
                                </b>
                            </td>
                            <td>
                                @item.CreatedOn.ToString("dd-MM-yyyy")
                            </td>
                            <td>
                                @item.Name
                            </td>


                            <td>

                                @Html.ActionLink("View", "ViewRequest", "JDE", new { TransactionID = item.ID, Status = item.Status, Mode = "V" }, new { @class = "btn btn-primary btn-sm" })
                            </td>



                        </tr>
                    }
                    </tbody>
                </table>
            </div>
        </div>

